{
  "name": "@kotolis/react-universal-svg",
  "version": "0.0.6",
  "description": "The flexible universal SVG component that renders with DOM and react-native (expo)",
  "main": "lib",
  "scripts": {
    "start:dom": "REACT_APP_ENV=development:dom react-scripts start",
    "start:native": "REACT_APP_ENV=development:native expo start",
    "start:native:ios": "REACT_APP_ENV=development:native expo start --ios",
    "start:native:android": "REACT_APP_ENV=development:native expo start --android",
    "build": "rm -rf lib && NODE_ENV=production babel --presets react-app --plugins=transform-node-env-inline,minify-dead-code-elimination -d lib src --ignore src/starters && mv lib/index.web.js lib/index.js && mv lib/index.native.js lib/index.ios.js && cp lib/index.ios.js lib/index.android.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kutran-dmitry/react-universal-svg.git"
  },
  "keywords": [
    "react",
    "react-native",
    "expo",
    "primitives",
    "svg"
  ],
  "author": "Dmitry Kutran <dmitrykutran@gmail.com> (https://github.com/kutran-dmitry)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kutran-dmitry/react-universal-svg/issues"
  },
  "homepage": "https://github.com/kutran-dmitry/react-universal-svg#readme",
  "dependencies": {
    "buffer": "^5.2.0",
    "camelcase": "^5.0.0",
    "dompurify": "^1.0.7",
    "events": "^3.0.0",
    "expo": "^29.0.0",
    "object.omit": "^3.0.0",
    "object.pick": "^1.3.0",
    "prop-types": "^15.6.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-primitives": "^0.6.0",
    "react-scripts": "^1.1.5",
    "react-xml-parser": "^1.0.7",
    "stream": "0.0.2",
    "timers": "^0.1.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-minify-dead-code-elimination": "^0.4.3",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
